{
  "name": "foodys",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test:e2e": "start-server-and-test dev http-get://localhost:3000 cypress",
    "cypress": "cypress run",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [ "lint" ],
     "*.vue": [ "lint" ]
 },
 "husky": { 
  "hooks": {
     "pre-commit": "lint-staged && yarn run test:unit"
  }
},
  "dependencies": {
    "element-ui": "^2.15.6",
    "vue": "^3.2.6",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^27.0.1",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.6",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^7.0.0",
    "prettier": "2.3.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.2",
    "vue-tsc": "^0.2.2"
  }
}
